pipeline {
    agent { 
     
            label 'jenkins'
    
      }
      triggers{
        pollSCM '*/5 * * * *'
      }
    stages {
            stage('Verify version') {
      steps {
        sh 'docker run -u 0 --privileged --name jenkins -it -d \-v /var/run/docker.sock:/var/run/docker.sock \-v $(which docker):/usr/bin/docker \-v /home/jenkins_home:/var/jenkins_home \flyway/flyway'
      }
    }
    }
}