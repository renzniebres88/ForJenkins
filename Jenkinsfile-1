pipeline {
    agent { 
     
            label 'jenkins'
    
      }
      triggers{
        pollSCM '*/5 * * * *'
      }
    stages {
            stage('Verify version') {
      steps {
        sh 'docker run
-v /var/run/docker.sock:/var/run/docker.sock
-v $(which docker):/usr/bin/docker
-v /home/jenkins_homes:/var/jenkins_homes
flyway/flyway'
      }
    }
    }
}